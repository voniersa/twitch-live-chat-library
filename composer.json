{
    "name": "voniersa/twitch-live-chat",
    "description": "A simple and easy to use library for reading from and writing to the twitch irc live chats by partially using the observer design pattern",
    "type": "library",
    "authors": [
        {
            "name": "Sascha Vonier",
            "email": "vonier1998@gmx.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "slevomat/coding-standard": "^7.2"
    },
    "license": "MIT",
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
